<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme --> <!-- standard, dark -->
        <theme>standard</theme>

        <!-- document encoding -->  <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>

        <!-- protocol for script and stylesheets -->   <!-- defaults to https -->
        <protocol>http</protocol>

        <!-- title of the document -->
        <documentTitle>Tests report - SafetyCube</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>Automated tests - SafetyCube</reportName>

        <!-- global date format override -->  <!-- defaults to yyyy-MM-dd -->
        <dateFormat>dd-MM-yyyy</dateFormat>

        <!-- global time format override -->   <!-- defaults to HH:mm:ss -->
        <timeFormat>HH:mm:ss</timeFormat>

        <!-- custom javascript -->
        <scripts>
            <![CDATA[
            $(document).ready(function() {
                $('.brand-logo.blue.darken-3').empty();
                $('.brand-logo.blue.darken-3').append('<img src=\"../media/img/logo-safetyline.jpg\" />');
                $('.brand-logo.blue.darken-3').addClass('white');
                $('.brand-logo.blue.darken-3').css('width','auto');
                $('.brand-logo.white.blue.darken-3').removeClass('blue');
                $('.brand-logo.white.darken-3').removeClass('darken-3');
                $('.brand-logo.white img').addClass('logo-vieroli');
            });
          ]]>
        </scripts>

        <!-- custom styles -->
        <styles>
            <![CDATA[
                .logo-vieroli{
                    height: 30px;
                    padding-right: 20px;
                    padding-top: 15px;
                }
            ]]>
        </styles>
    </configuration>
</extentreports>